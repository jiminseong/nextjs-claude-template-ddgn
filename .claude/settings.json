{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },
    "vercel": {
      "transport": {
        "type": "http",
        "url": "https://mcp.vercel.com/"
      },
      "environment": {
        "VERCEL_TOKEN": "${env:VERCEL_TOKEN}"
      }
    }
  }
}
